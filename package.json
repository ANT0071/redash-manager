{
  "name": "redash-manager",
  "version": "1.0.0",
  "repository": "https://github.com/caugner/redash-manager",
  "license": "MPL-2.0",
  "description": "Download and manage Redash queries locally",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "prepare": "test -f .env || cp .env-dist .env && lefthook install",
    "download": "node --env-file=.env src/index.js download",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "redash",
    "query",
    "management"
  ],
  "author": "",
  "devDependencies": {
    "@types/node": "^24.9.1",
    "lefthook": "^1.10.3",
    "prettier": "^3.4.2",
    "typescript": "^5.7.0"
  }
}
